<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Explore Market</title>
    <style>
        body { font-family: Arial, sans-serif; margin: 20px; }
        .data-container { margin-top: 20px; }
        button { padding: 10px 20px; margin-right: 10px; cursor: pointer; }
        .data-point { margin-bottom: 10px; }
        .timestamp { font-weight: bold; }
    </style>
</head>
<body>
    <h1>Market Data</h1>
    <ul>
        <% for(const [key, value] of Object.entries(marketData)) { %>
            <li><%= key %>: <%= JSON.stringify(value) %></li>
        <% } %>
    </ul>
    <form action="/save-data" method="POST">
        <input type="hidden" name="timestamp" value="<%= new Date().toISOString() %>">
        <input type="hidden" name="data" value='<%= JSON.stringify(marketData) %>'>
        <button type="submit">Save Data</button>
    </form>
</body>
</html>
